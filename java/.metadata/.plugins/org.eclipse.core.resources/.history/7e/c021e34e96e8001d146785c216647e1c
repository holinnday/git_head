package chapter07;

public class LocalValEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Local local = new Local();
		
		System.out.println(local.name);
		
		local.process();
		System.out.println(local.name);

		local.printAge1();
		local.printAge2();
		
		int temp = 0;
		
		for (int i=0; i<10; i++) {
			
			temp += i;
		}
		
		System.out.println(temp);
	}

}

class Local {
	
	String name;
	
	void process() {
		name = "홍길동";
	}
	void printAge1() {
		int age = 20;
		System.out.println(age);
	}
	void printAge2() {
		int age = 30;
		System.out.println(age);
	}
}